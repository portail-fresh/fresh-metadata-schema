<?xml version="1.1" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="pp" vc:minVersion="1.1"
    xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning">

    <!-- root element-->
    <xsd:annotation>
        <xsd:documentation xml:lang="fr">Description du bras</xsd:documentation>
        <xsd:documentation xml:lang="en">Blinded masking, details</xsd:documentation>
    </xsd:annotation>
    <xsd:element name="FreshSchema">
        <xsd:annotation>
            <xsd:documentation xml:lang="fr">
                Schema Métadonnées FReSH
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <!-- to say that the element contains other elements-->
            <xsd:sequence>
                <!-- ensures child elements appear in a defined order -->
                <xsd:element name="TechnicalInfo">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="fr">Métadonnées techniques de la fiche</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="1" minOccurs="1" name="StudyId"
                                type="DatasetIDType">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Identifiant de l'étude</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Study ID</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="AcquisitionMode">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Mode d'acquisition de la fiche de metadonnées</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Acquisition mode of the metadata sheet</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="IsImport" type="xsd:boolean">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Import?</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="Provenance" default="PEF" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Provenance
</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Provenance</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="clinicaltrials.gov"/>
                                                  <xsd:enumeration value="CTIS"/>
                                                  <xsd:enumeration value="PROGEDO"/>
                                                  <xsd:enumeration value="ECLAIRE"/>
                                                  <xsd:enumeration value="PEF"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element default="fr" name="VersionLang">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Langue de la version</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Version language</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="fr"/>
                                        <xsd:enumeration value="en"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element default="fr" name="OriginLang">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Langue d'origine</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Original language</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="fr"/>
                                        <xsd:enumeration value="en"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="CreationDate" type="xsd:date">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Date du 1er enregistrement</xsd:documentation>
                                    <xsd:documentation xml:lang="en">First record date</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="LastUpdatedAuto" type="xsd:date">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Date de la dernière MAJ automatique</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Last automatic update date</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="LastUpdatedManual" type="xsd:date">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Date de la dernière MAJ manuelle</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Last manual update date</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="RespValidation" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation>Completion/validation par le responsable</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="AutoTranslation" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Traduction automatique</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Completion/validation by the responsible person</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="Status">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Status</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Status</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="Brouillon"/>
                                        <xsd:enumeration value="En attente de validation"/>
                                        <xsd:enumeration value="Retournée"/>
                                        <xsd:enumeration value="Rejetée"/>
                                        <xsd:enumeration value="Publiée"/>
                                        <xsd:enumeration value="Importée"/>
                                        <xsd:enumeration value="Mise à jour automatiquement"/>
                                        <xsd:enumeration value="Mise à jour manuellement"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherStudyId"
                                type="DatasetIDType">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Autre identifiant de l'étude</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="MetadataContributor">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Contributeur de la fiche de métadonnées</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Metadata Contributor</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="ContributorName" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Nom-Prénom</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Last name – First name</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element minOccurs="0" name="ContributorAffiliation">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Affiliation</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Affiliation</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="CollectionContext">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="fr">Renseignements sur le contexte de la collecte des données</xsd:documentation>
                        <xsd:documentation xml:lang="en">Context of data collection</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="AdministrativeInformation">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Renseignements administratifs</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Administrative information</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="General">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Général</xsd:documentation>
                                                <xsd:documentation xml:lang="en">General</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element maxOccurs="1" minOccurs="1"
                                                  name="Title" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Titre</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Title</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="Acronym" type="xsd:string"
                                                  minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Acronyme</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Acronym</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="RegulatoryRequirements"
                                                  minOccurs="1">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Pré-requis réglementaires</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Regulatory requirements</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="ObtainedAuthorization"
                                                  maxOccurs="unbounded" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Autorisations ou avis obtenus</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Authorizations or approvals obtained</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="AuthorizingAgency">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Autorité competente</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Competent authority</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="ANSM"/>
                                                  <xsd:enumeration value="CNIL"/>
                                                  <xsd:enumeration value="CESREES"/>
                                                  <xsd:enumeration value="Autre"/>
                                                  <xsd:enumeration value="CPP"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="OtherAuthorizingAgency"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Autre, précision</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Other, specify</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="0"
                                                  name="ConformityDeclaration">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Declaration de conformité</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="MR001"/>
                                                  <xsd:enumeration value="MR002"/>
                                                  <xsd:enumeration value="MR003"/>
                                                  <xsd:enumeration value="MR004"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded"
                                            name="PrimaryInvestigator">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Investigateur principale</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Principal Investigator</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="PIName" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="Affiliation">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="OrganisationName"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom Organisme d'affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Identifiant de l'organisme d'affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="OrganisationPID"
                                                  type="OrganisationPIDType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Identifiant de l'organisme d'affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Identifiant d'organisme d'affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="PersonPID" type="PersonPIDType"
                                                  minOccurs="0" maxOccurs="unbounded">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">ID Personne</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Person ID</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" name="Contributor"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Membre de l'équipe de recherche</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Research team member</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="ContributorName"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="Affiliation">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="OrganisationName"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom Organisme d'affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Identifiant de l'organisme d'affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="OrganisationPID"
                                                  type="OrganisationPIDType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Identifiant de l'organisme d'affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Identifiant d'organisme d'affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="PersonPID" type="PersonPIDType"
                                                  maxOccurs="unbounded" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="en">Person ID</xsd:documentation>
                                                  <xsd:documentation xml:lang="fr">ID personne</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="ContactPoint">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Point de contact</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="ContactName" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="Affiliation">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="OrganisationName"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom Organisme d'affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Identifiant de l'organisme d'affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="OrganisationPID"
                                                  type="OrganisationPIDType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Identifiant de l'organisme d'affiliation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Identifiant d'organisme d'affiliation</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="EMail" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">E-mail</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">E-mail</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="FundingAgent" maxOccurs="unbounded"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Financement</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="FundingAgentName"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom Financeur</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Funder name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="FundingAgentType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Type Financeur</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Funder type</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Public (France)"/>
                                                  <xsd:enumeration value="Public (Europe)"/>
                                                  <xsd:enumeration value="Industrie"/>
                                                  <xsd:enumeration value="Privé à but non lucratif"/>
                                                  <xsd:enumeration value="Autre"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="FundingAgentPID"
                                                  type="OrganisationPIDType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">ID Financeur</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Funder ID</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="OrganisationGovernance">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Organisation et gouvernance</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Organisation and governance</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="Sponsor">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Promoteur/Organisme responsable</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Sponsor / Responsible organisation</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="SponsorName" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom Promoteur</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Spoonsor name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="SponsorType" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Type Promoteur</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Sponsor type</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="SponsorPID"
                                                  type="OrganisationPIDType" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">ID Promoteur</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Sponsor ID</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="Governance" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Gouvernance</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Governance</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="Committee" type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Comité scientifique ou pilotage</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Scientific or steering committee</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="CommitteeDetail"
                                                  type="xsd:string" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Comité, précisions</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Committee details</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="OtherGovernance"
                                                  type="xsd:string" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Autre, précisions</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Other, specify </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="Collaborations" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Collaborations</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collaborations</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="NetworkConsortium"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Réseaux, consortiums</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Networks, consortia</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="CollaborationsDetail"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Précisions</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Details</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="StudyStatus">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Statut de l'étude</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Study status</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="Etude active"/>
                                        <xsd:enumeration value="Etude completée"/>
                                        <xsd:enumeration value="Etude arretée"/>
                                        <xsd:enumeration value="Incunnu"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="Theme">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Thématique</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Theme</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Purpose" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Objectifs</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Objectives</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="HealthTheme" type="CvIdType">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Specialité medicale</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Medical field</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="Pathology" type="CvIdType">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Groupe de pathologies </xsd:documentation>
                                                <xsd:documentation xml:lang="en">Medical field</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="Keyword" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Mot-clé libre</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Keyword</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="HealthDeterminant" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Déterminant de santé</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Health determinant</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element maxOccurs="1" minOccurs="1" name="Summary"
                                            type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Résumé</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Summary </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="ComplementaryInformation"
                                            type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Information complémentaire précisant l'étude</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Complementary information specifying the study</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="StudyMethodology">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="fr">Méthodologie de l'étude</xsd:documentation>
                        <xsd:documentation xml:lang="en">Study methodology</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="StudySchema">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Schéma d'étude</xsd:documentation>
                                    <xsd:documentation xml:lang="en">StudyDesign</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="ResearchType">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Type de recherche</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Study type</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Observationnelle"/>
                                                  <xsd:enumeration
                                                  value="Interventionnelle (expérimentale)"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="AnalysisUnit" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Unité d'analyse</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Analysis unit</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="InterventionalStudy" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Etude interventionnelle (expérimentale)</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Interventional study</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="ResearchPurpose"
                                                  maxOccurs="unbounded">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Objet principal de la recherche</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Research purpose</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Traitement"/>
                                                  <xsd:enumeration
                                                  value="Recherche sur les services de santé"/>
                                                  <xsd:enumeration value="Prévention"/>
                                                  <xsd:enumeration value="Recherche fondamentale"/>
                                                  <xsd:enumeration value="Diagnostic"/>
                                                  <xsd:enumeration
                                                  value="Faisabilité des dispositifs"/>
                                                  <xsd:enumeration value="Soins de soutien"/>
                                                  <xsd:enumeration value="Dépistage"/>
                                                  <xsd:enumeration value="Autre"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="TrialPhase"
                                                  maxOccurs="unbounded">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Phase d'essai</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Stduy phase</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Phase 1"/>
                                                  <xsd:enumeration value="Phase 2"/>
                                                  <xsd:enumeration value="Phase 3"/>
                                                  <xsd:enumeration value="Phase 4"/>
                                                  <xsd:enumeration value="N/A"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="InterventionalStudyModel">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Schema d'intervention</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Intervention model</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Simple bras / Mono-bras"/>
                                                  <xsd:enumeration value="Groupes parallèles"/>
                                                  <xsd:enumeration
                                                  value="Schema croisé / Etudes croisées"/>
                                                  <xsd:enumeration value="Plan factoriel"/>
                                                  <xsd:enumeration value="Séquentiel"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="Allocation">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Allocation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Allocation</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="AllocationMode">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Mode d'allocation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Allocation type</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Randomisé"/>
                                                  <xsd:enumeration value="Non randomisé"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="AllocationUnit">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Unité d'allocation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Allocation unit</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Individuelle"/>
                                                  <xsd:enumeration value="Cluster"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="Masking">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Insu</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Masking</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="MaskingType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Type insu</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Masking type</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="En aveugle"/>
                                                  <xsd:enumeration value="En ouvert"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="BlindedMaskingDetail"
                                                  maxOccurs="unbounded">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Insu en aveugle, précisions</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Blinded masking, details</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Patient"/>
                                                  <xsd:enumeration value="Equipe soignante"/>
                                                  <xsd:enumeration value="Evaluateur des résultats"/>
                                                  <xsd:enumeration value="Equipe de recherche"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="Arms">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Bras / groupe</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Arms / groups</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="ArmsNumber"
                                                  type="xsd:positiveInteger">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nombre de bras</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Number of arms</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="1"
                                                  name="Arm">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Ajouter un bras</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Arm</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="ArmType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Type du bras</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Type de bras</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="ExperimentalArm"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Expérimental</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="ActiveComparatorArm"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Comparateur actif</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="PlaceboComparatorArm"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Comparateur placebo</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="SharmComparatorArm"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Comparateur fictif</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="NoInterventionArm"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Sans intervention</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="ArmName" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Nom du bras
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="ArmDescription"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Description du bras
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="ObservationalStudy" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Observationnelle</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Observational study</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="ObservationalStudyDesign">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Modèle de l'étude observationnelle</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Observational Study  Design</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Registres"/>
                                                  <xsd:enumeration value="Longitudinale ou cohorte"/>
                                                  <xsd:enumeration value="Etudes transversales"/>
                                                  <xsd:enumeration value="Etudes cas-temoin"/>
                                                  <xsd:enumeration value="Autres"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="CohortLongitudinal"
                                                  minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Etude longitudinale ou cohorte</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Longitudinal or cohort study</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="RecrutementTiming">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Temporalité du recrutement</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Recruitment Timing</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration
                                                  value="Avec recrutement rétrospectif"/>
                                                  <xsd:enumeration
                                                  value="Avec recrutement prospectif"/>
                                                  <xsd:enumeration
                                                  value="Avec recrutement prospectif et rétrospectif"/>
                                                  <xsd:enumeration value="Autre"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="OtherStudyCategory"
                                                  maxOccurs="1" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Autre modèle de recherche</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">other study design</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="OtherStudyCategoryDetails"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Précisions</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="InclusionGroups">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Groupes à l'inclusion</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Inclusion groups</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="NrInclusionGroupes"
                                                  type="xsd:positiveInteger">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nombre de groupes</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Number of groups</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="0"
                                                  name="InclusionGroup">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Groupe à l'inclusion
</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Inclusion group</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="GroupName" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom du groupe</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Groupe name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="GroupDescription"
                                                  type="xsd:string" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Description du groupe</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Group description</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="GroupInterventionExposition"
                                                  type="xsd:string" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">exposition associée au groupe</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Exposition associated to the group</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="Intervention">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Intervention/exposition</xsd:documentation>
                                                <xsd:documentation xml:lang="en"/>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="InterventionName"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom de l'intervention/exposition</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Intervention/exposition name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="InterventionType" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Type d'intervention
</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Intervention/exposition type</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Medicament"/>
                                                  <xsd:enumeration value="Dispositif medical"/>
                                                  <xsd:enumeration value="Biologique/vaccin"/>
                                                  <xsd:enumeration value="Procédure/chirurgie"/>
                                                  <xsd:enumeration value="Radiation"/>
                                                  <xsd:enumeration value="Comportamentale"/>
                                                  <xsd:enumeration value="Génétique"/>
                                                  <xsd:enumeration value="Complement alimentaire"/>
                                                  <xsd:enumeration value="Test diagnostique"/>
                                                  <xsd:enumeration value="Combinaison"/>
                                                  <xsd:enumeration value="Santé publique"/>
                                                  <xsd:enumeration value="Autres"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="InterventionDescritption"
                                                  type="xsd:string" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Description</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Intervention/exposition description</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DataCollectionIntegration">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Collecte et réutilisation de données</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Data collection and reuse</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="DataCollection">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Données individuelles produites ou collectées dans le cadre de l'étude</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Individual data produced or collected as part of the study</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="CollectionProcess">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Procedure de collecte</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collection procedure</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="CollectionFrequency"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Fréquence de la collecte</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collection frequency</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="CollectionMode"
                                                  maxOccurs="unbounded" minOccurs="1"
                                                  type="CvIdType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Mode de collecte</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collection mode</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element minOccurs="0"
                                                  name="CollectionModeDetails">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Mode de collecte, précisions</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collection mode, details</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="CollectionChronology">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Chronologie de la collecte</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collection chronology</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="CollectionStart"
                                                  type="xsd:date">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Date de début de la collecte (recrutement du 1er participant)</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collection start date</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="CollectionEnd" type="xsd:date">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Date de fin de la collecte (dernier suivi du dernier participant)</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Collection end date</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="InclusionStrategy"
                                                  maxOccurs="unbounded">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Stratégie d’inclusion</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Inclusion strategy</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration
                                                  value="Inclusion par exposition/facteur (selon une ou des variables individuelles d'intérêt)"/>
                                                  <xsd:enumeration
                                                  value="Inclusion basée sur une caractéristique individuelle (critère clinique, comportemental ou socio-démographique)"/>
                                                  <xsd:enumeration
                                                  value="Inclusion fondée sur un événement passé (de santé, de parcours, d'une étape de vie...) ou critère de jugement (outcome-based)"/>
                                                  <xsd:enumeration
                                                  value="Inclusion par appartenance à un cadre externe : contexte organisationnel ou territorial (Personnes vivant dans un lieu, appartenant à une situation organisationnelle donnée, usagers d’un service)"/>
                                                  <xsd:enumeration
                                                  value="Inclusion par participation volontaire"/>
                                                  <xsd:enumeration value="Autre"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="SamplingMode"
                                                  maxOccurs="unbounded" type="CvIdType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Procédure d'échantillonnage à l'inclusion</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Sampling procedure at inclusion</xsd:documentation>
                                                  </xsd:annotation>
