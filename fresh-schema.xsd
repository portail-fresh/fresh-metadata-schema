                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="StatisticalPopulationBases"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Bases de population à finalité statistique</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="HealthEventRegistry"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Registre de maladies, de décès</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="OtherBases" type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Autre base</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="OtherBasesDetails"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Autre base, précisions</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="ActiveFollowUp" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Suivi actif des participants</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Active follow up</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="IsActiveFollowUp"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Suivi actif des participants?</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Is active followup?</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="FollowUptMode"
                                                  maxOccurs="unbounded">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Modalité de suivi</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Follow-up method</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration
                                                  value="Suivi par contact avec le participant "/>
                                                  <xsd:enumeration
                                                  value="Suivi par visite du participant au centre investigateur"/>
                                                  <xsd:enumeration value="Autre"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="IsDataIntegration" type="xsd:boolean">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Utilisation des données individuelles issues d'autres sources des données</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Use of Individual Data from Other Data Sources</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DataIntegration">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Utilisation des données individuelles issues d'autres sources des données</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="0"
                                                  name="ThirdPartySource">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Source</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Source</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element name="SourceName" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Nom source</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Source name</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="SourcePurpose">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Objectif de l'integration de la source</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Source integration purpose</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Ajout d'individus"/>
                                                  <xsd:enumeration
                                                  value="Enrichissement par croisement"/>
                                                  <xsd:enumeration
                                                  value="Suivi passif des participants"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="SourceType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Type de source</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Souce type</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Sources cliniques"/>
                                                  <xsd:enumeration
                                                  value="Sources paracliniques (hors biologiques)"/>
                                                  <xsd:enumeration value="Biobanques"/>
                                                  <xsd:enumeration value="Registres de morbidité"/>
                                                  <xsd:enumeration value="Sources administratives"/>
                                                  <xsd:enumeration value="Données de recherche"/>
                                                  <xsd:enumeration value="Autres"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="OtherSourceType"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Autre type de source, précisions</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Other sourcetype, details</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Population">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Population</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Population</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="DemographicInfo">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Caractéristiques démographiques</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Demographic characteristics</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="Sex" maxOccurs="unbounded"
                                                  minOccurs="1" type="CvIdType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Sexe</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Sex</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="Age" maxOccurs="unbounded"
                                                  type="CvIdType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Age</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Age</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="PopulationType" maxOccurs="unbounded"
                                            type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Type de population</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="OtherPopulationType" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="en">Other population type, details</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="OtherClusion">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Autres critères d'éligibilité</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Other eligibility criteria</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="0"
                                                  name="InclusionCriterion" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Critère d'inclusion</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Inclusion criterion</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="0"
                                                  name="ExclusionCriterion" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Critère d'exclusion</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Exclusion criterion</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="GeographicalCoverage">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Champ géographique</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Geographical coverage</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="1"
                                                  name="Nation" type="CvIdType">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Pays concerné</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Country concerned</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element maxOccurs="unbounded" minOccurs="0"
                                                  name="FranceRegion" type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Region concerné (en France)</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Region concerned (France)</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="GeoDetail" type="xsd:string"
                                                  minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Détail du champ géographique</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Geographical coverage details</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Outcomes">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Paramètres de santé étudiés</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Health-related outcomes</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PrimaryOutcomes" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Critères d'évaluation (de jugement) principaux</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Primary Outcome</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="SecondaryOutcomes" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Critères d'évaluation (de jugement) secondaire</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Secondary Outcomes</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="DataCharacteristics">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="fr">Caractéristiques des données collectées</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SampleSize">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Nombre de participants</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Number of participants</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PlannedSampleSize" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Nombre prévu</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Planned number</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FinalSampleSize" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Effectif réel</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Actual number</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DataQuality">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Qualité des données</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Data quality</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="UsedStandards" maxOccurs="unbounded"
                                            minOccurs="0" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Standards ou nomenclatures employés</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Standards or nomenclatures used</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="QualityProcedure" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Procédure qualité</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="VariableDictionnary">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Dictionnaire des variables</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Data dictionary</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="VariableDictionnaryAvailable"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Existence d'un dictionnaire des variables</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Presence of a data dictionary</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="VariableDictionnaryDetails"
                                                  type="xsd:string" minOccurs="0">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Lien vers le dictionnaire des variables (si accessible)</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Link to the data dictionary (if accessible)</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="OtherDocumentation" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Autres documentations sur les données</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Other data documentation</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DataAccess">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Accès aux données</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Data access</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="IndividualDataAccess" type="CvIdType">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Accès aux données individuelles</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Access to individual data</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="AggregatedDataAccess" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Accès aux données agrégées</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Access to aggregated data</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="AccessConditions" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Condition d'accès aux données</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Data access conditions</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="AdditionalDataAccessLink"
                                            type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Lien vers informations complémentaires relatives à l'accès aux données</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Link to additional information on data access</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="NonDisclosureAgreement" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Accord de confidentialité</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Non-disclosure agreement</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="AccessRestrictions" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Restrictions d'accès</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Access restrictions</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DataLocation" type="xsd:string"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Localisation des données</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Data location</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DataInformationContact" type="xsd:string"
