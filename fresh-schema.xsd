                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Obligation de citation</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Citation requirement</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="MockSample">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Echantillon fictif</xsd:documentation>
                                                <xsd:documentation>Mock sample</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="MockSampleAvailable"
                                                  type="xsd:boolean">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Existence d'un échantillon fictif


</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Availability of a mock sample</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="MockSampleLocation"
                                                  type="xsd:string">
                                                  <xsd:annotation>
                                                  <xsd:documentation xml:lang="fr">Lien ou préciser</xsd:documentation>
                                                  <xsd:documentation xml:lang="en">Link or specify</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
