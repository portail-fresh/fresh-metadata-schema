                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Données biologiques, précisions</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Biological data details</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="isDataInBiobank" type="xsd:boolean"
                                            minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Présence des échantillons dans  une biobanque</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BiobankContent" maxOccurs="unbounded">
                                            <xsd:annotation>
                                                <xsd:documentation xml:lang="fr">Contenu de la biobanque</xsd:documentation>
                                                <xsd:documentation xml:lang="en">Biobank content</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="RelatedDocuments">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="fr">Documents connexes</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0"
                                name="UsingDataPublication" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Publications utilisant le recueil des données</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element maxOccurs="unbounded" minOccurs="0"
                                name="ProtocolPublication" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Publication définissant la base de données et son protocole, lien vers le document</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element maxOccurs="unbounded" minOccurs="0"
                                name="OutcomePublication" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="fr">Publications décrivant les résultats (articles, posters, rapports, ouvrages ou chapitres...)</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>

        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="CvIdType">
        <xsd:sequence>
            <xsd:element default="" name="value" type="xsd:string"/>
            <xsd:element name="URI" type="xsd:anyURI"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PersonPIDType">
        <xsd:sequence>
            <xsd:element name="URI" type="xsd:anyURI"/>
            <xsd:element name="PIDSchema">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ORCID"/>
                        <xsd:enumeration value="IdRef"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="OrganisationPIDType">
        <xsd:sequence>
            <xsd:element name="URI" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">URI organisme</xsd:documentation>
                    <xsd:documentation xml:lang="en">Organisation identifier URI</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PIDSchema">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">Type d'identifiant organisme (ROR, SIRENE, RNSR)</xsd:documentation>
                    <xsd:documentation xml:lang="en">Organisation identifier type (ROR, SIRENE, RNSR)</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ROR"/>
                        <xsd:enumeration value="RNSR"/>
                        <xsd:enumeration value="SIRENE"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DatasetIDType">
        <xsd:sequence>
            <xsd:element name="Identifier" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">Code de l'identifiant</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="URI" type="xsd:anyURI" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">URI de l'identifiant</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IDSchema" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">Source de l'identifiant</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>

    </xsd:complexType>
</xsd:schema>
